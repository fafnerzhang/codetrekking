# ===============================
# CodeTrekking Environment Configuration
# ===============================

# Project Root Paths
ROOT=/home/aiuser/codetrekking
DOCKER_DIR=/home/aiuser/codetrekking/docker

# Stack Configuration
DEFAULT_STACK_NAME=codetrekking
ELK_STACK_NAME=elk
AIRFLOW_STACK_NAME=airflow
PORTAINER_STACK_NAME=portainer

# ===============================
# Port Configurations
# ===============================
# Elasticsearch Stack
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_TRANSPORT_PORT=9300
KIBANA_PORT=5601
LOGSTASH_PORT=5044
LOGSTASH_TCP_PORT=5001
LOGSTASH_UDP_PORT=5001
LOGSTASH_GELF_PORT=12201
LOGSTASH_API_PORT=9600

# Portainer Management
PORTAINER_WEB_PORT=9000
PORTAINER_HTTPS_PORT=9443
PORTAINER_EDGE_PORT=8000

# Docker Registry
REGISTRY_PORT=5000

# RabbitMQ
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# PostgreSQL
POSTGRES_PORT=5432

# MinIO S3 Storage
MINIO_API_PORT=9100
MINIO_CONSOLE_PORT=9101

# API Service
API_SERVICE_PORT=9020

# ===============================
# Elasticsearch Configuration
# ===============================
ELASTIC_VERSION=9.0.1

ELASTICSEARCH_HOST=http://elasticsearch:9200
ELASTICSEARCH_USER=elastic
ELASTIC_PASSWORD=ChangeMe

# User 'logstash_internal' (custom)
#
# The user Logstash uses to connect and send data to Elasticsearch.
# https://www.elastic.co/guide/en/logstash/current/ls-security.html
LOGSTASH_INTERNAL_PASSWORD=ChangeMe

# User 'kibana_system' (built-in)
#
# The user Kibana uses to connect and communicate with Elasticsearch.
# https://www.elastic.co/guide/en/elasticsearch/reference/current/built-in-users.html
KIBANA_SYSTEM_PASSWORD=ChangeMe

# User 'filebeats_system'
FILEBEAT_INTERNAL_PASSWORD=ChangeMe
BEATS_SYSTEM_PASSWORD=ChangeMe

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_VHOST=codetrekking
RABBITMQ_DEFAULT_USER=codetrekking
RABBITMQ_DEFAULT_PASS=ChangeMe
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_URL=amqp://codetrekking:ChangeMe@localhost:5672/codetrekking

# ===============================
# Airflow Configuration
# ===============================
AIRFLOW_DEPLOY_NODE_ROLE=manager
AIRFLOW_VERSION=2.9.3
AIRFLOW_USER=airflow
AIRFLOW_PASSWORD=ChangeMe
AIRFLOW_POSTGRES_PASSWORD=ChangeMe
AIRFLOW_UID=1000
AIRFLOW_GID=0
AIRFLOW_PROJ_DIR=/home/aiuser/codetrekking/application/airflow
AIRFLOW_EMAIL=changeme@example.com

# ===============================
# API Service Configuration
# ===============================
API_HOST=0.0.0.0
API_PORT=8002
API_SERVICE_PORT=8002
API_SERVICE_VERSION=latest
ENVIRONMENT=development
LOG_LEVEL=DEBUG

# JWT Configuration
JWT_SECRET_KEY=change-this-secret-key-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://127.0.0.1:3000,http://127.0.0.1:8080

# ===============================
# Docker Registry Configuration
# ===============================
REGISTRY_URL=localhost:5000

# ===============================
# PeakView Frontend Configuration
# ===============================
# OpenAI API Configuration
OPENAI_API_KEY=YOUR_API_KEY
LANGCHAIN_CALLBACKS_BACKGROUND=false

# Required for agent example
SERPAPI_API_KEY=YOUR_API_KEY

# Required for retrieval examples
SUPABASE_PRIVATE_KEY=YOUR_SUPABASE_PRIVATE_KEY
SUPABASE_URL=YOUR_SUPABASE_URL

# Optional: For Tracing with LangSmith
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=YOUR_API_KEY
LANGCHAIN_PROJECT=nextjs-starter

# Optional: Other model keys
ANTHROPIC_API_KEY=YOUR_API_KEY

# Turn on demo mode
NEXT_PUBLIC_DEMO=false

# ===============================
# PeakFlow Configuration
# ===============================
# Garmin Data Storage
GARMIN_CONFIG_DIR=/storage/garmin
GARMIN_DATA_DIR=/storage/garmin

# Garmin Credentials Encryption
GARMIN_ENCRYPTION_KEY=<REPLACE_WITH_GENERATED_KEY>
GARMIN_ENCRYPTION_VERSION=1

# ===============================
# PeakFlow Tasks Configuration
# ===============================
# Celery Configuration (uses RabbitMQ settings above)
WORKER_SERVICE_VERSION=latest
WORKER_CONCURRENCY=4
WORKER_LOG_LEVEL=INFO

# Database Configuration for Tasks (Phase 6)
DATABASE_URL=postgresql://codetrekking:${POSTGRES_PASSWORD}@postgres:5432/codetrekking
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=codetrekking
POSTGRES_USER=codetrekking

# ===============================
# Storage Paths
# ===============================
STORAGE_ROOT=/home/aiuser/codetrekking/storage
GARMIN_STORAGE=/home/aiuser/codetrekking/storage/garmin
ELASTICSEARCH_STORAGE=/home/aiuser/codetrekking/storage/elasticsearch
AIRFLOW_POSTGRES_STORAGE=/home/aiuser/codetrekking/storage/airflow_postgres
PORTAINER_STORAGE=/home/aiuser/codetrekking/storage/portainer_data
RABBITMQ_STORAGE=/home/aiuser/codetrekking/storage/rabbitmq_data
DOCKER_REGISTRY_STORAGE=/home/aiuser/codetrekking/storage/docker_registry

# ===============================
# PostgreSQL Configuration
# ===============================
POSTGRES_VERSION=15.4
POSTGRES_PORT=5432
POSTGRES_DB=codetrekking
POSTGRES_USER=codetrekking
POSTGRES_PASSWORD=ChangeMe
POSTGRES_STACK_NAME=postgres
POSTGRES_STORAGE=/home/aiuser/codetrekking/storage/postgres_data

